.wx_end_container {
  position: fixed;
  top: -300rpx;
  width: 80rpx;
  height: 300rpx;
  right: 20rpx;
  animation: tt 1s forwards;
  z-index: 99999999999;
}

.gg {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 150rpx;
  border-radius: 50%;
}

.line {
  width: 3rpx;
  height: 200rpx;
  background: #639CEC;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 0rpx;
}

@keyframes tt {
  0% {
    top: -300rpx
  }

  100% {
    top: -30rpx;
  }
}